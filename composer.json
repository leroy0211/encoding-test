{
    "name": "tidi/text-encoder",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "phpunit/phpunit": "^7.0",
        "symfony/flex": "^1.0",
        "symfony/twig-bundle": "^4.1",
        "symfony/form": "^4.1",
        "symfony/yaml": "^4.1",
        "symfony/framework-bundle": "^4.1"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "symfony/dotenv": "^4.1"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
